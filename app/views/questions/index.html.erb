<h1>Main page motherfucker</h1>
  
  <ul>
    <% @questions.each do |question|%> 
      <% user = question.user_id %>
        <% if user %>
          <h3><li><%= image_tag Gravatar.new(User.find(user).email).image_url %>Question: <%= link_to question.title, question_path(question)%></li></h3>
      <% end %> 
    <% end %>
  </ul>
